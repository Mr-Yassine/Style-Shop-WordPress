---------------------------*/
.wpcf7 input[type="text"],
.wpcf7 input[type="email"],
.wpcf7 input[type="tel"],
.wpcf7 textarea {
	width: 100%;
	font-size: 14px;
	padding: 10px 20px;
}

.wpcf7 label,
.wpcf7 input[type="submit"] {
	float: left;
}

/* Contact Form 7 Submit Button 
-------------------------------*/

.wpcf7 input[type="submit"] {
    color: #000000;
    font-size: 16px;
    font-weight: 900;
		font-style: italic;
		padding: 5px 20px;
		margin-top: 10px;
	background-color: #c7e5ef;
}

.wpcf7 input:hover[type="submit"] {
    background: #a2a2a2;
    transition: all 0.4s ease 0s;
}

/*
 * Footer contact form
*/
.footer-contact {
	position: relative;
}
.submit-contact-footer {
	position: absolute;
	top: 0;
	right: 0;
	margin-top: 0 !important;
	font-size: 0 !important;
	background-repeat: no-repeat !important;
	background-color: transparent !important;
	background-position: center !important;
	border: 0 !important;
	padding: 8px 20px !important;
	width: 60px !important;
}
.email-contact-footer {
	background-color: #c7e5ef;
}
.list-menu-footer a {
	color: #666666;
}
.list-menu-footer a:hover {
	color: #65acc4;	
}
.topbar-right {
	display: flex;
}
.topbar-right_wrapper span {
	color: #4c4c4c;
	font-size: 15px;
	font-family: "Times New Roman", Sans-serif;
}
.topbar-right_wrapper select {
	color: #000000;
	font-size: 15px;
	background-color: transparent;
	border: none;
	font-family: "Times New Roman", Sans-serif;
}
.topbar-right_wrapper:first-child {
	margin-right: 20px;
	
}
@media ( min-width: 992px ) {
